[run]
branch = True
omit =
    tests_guide/migrations/*
    tests_guide/tests/*
    tests_guide/version.py
source = tests_guide
[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
fail_under = 100
show_missing = 1